#include<stdio.h>
#include<conio.h>
int main()
{
float S,V,T;
printf("\t=Menghitung jarak tempuh=\n");
printf("\t=========================\n\n");
printf("Masukkan kecepatan mobil = ");
scanf("%f",&V);
printf("Masukkan waktu tempuh = ");
scanf("%f",&T);
S = V * T;
printf("Maka jarak yang ditempuh adalah = %f",S);
return 0;
}
